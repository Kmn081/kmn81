                                                                            ten[count-1].append(ten_J_day[i-1][p])
                                                                            ten_day_shift[i-1].append(ten[count-1])
                                                                            
                                            
                                            
                                            
                                            
                                            
                                            
                                            
                                            
                                            
                
                
                
            
            
            
            
            
            
            
            
            
            
