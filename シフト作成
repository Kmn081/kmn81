import openpyxl
from openpyxl import Workbook
wb = openpyxl.load_workbook('shift3.xlsx')
sheet = wb.get_sheet_by_name('Sheet1')


member_name = []
date = []
day_of_the_week = {}
for row_of_cell_objects in sheet['A4':'A29']:
    for cell_obj in row_of_cell_objects:
        member_name.append(cell_obj.value)
for row_of_cell_objects in sheet['F3':'T3']:
    for cell_obj in row_of_cell_objects:
        date.append(cell_obj.value)
        
day_week = []
for row_of_cell_objects in sheet['F2':'T2']:
    for cell_obj in row_of_cell_objects:
        day_week.append(cell_obj.value)  
for i in range(15):
            day_of_the_week[date[i]] = day_week[i]

member_name_N = {}
member_name_S = {}
member_name_W = {}
member_name_R = {}
for i in range(26):
    member_name_N[member_name[i]] = sheet['B' + str(i+4)].value
for i in range(26):
    member_name_S[member_name[i]] = sheet['C' + str(i+4)].value    
for i in range(26):
    member_name_W[member_name[i]] = sheet['D' + str(i+4)].value
for i in range(26):
    member_name_R[member_name[i]] = sheet['E' + str(i+4)].value   

hope_shift = list(range(sheet.max_column - 1))

                  
for i in range(sheet.max_column - 1):
    hope_shift[i] = []
    for row_of_cell_objects in sheet['F' + str(i+4):'T' + str(i+4) ]:
        for cell_obj in row_of_cell_objects:  
            hope_shift[i].append(cell_obj.value)
            
date_shift = list(range(15))
for i in range(15):
    date_shift[i] = []   
for i in range(0,15):    
    for column in sheet.iter_rows(min_row=4, min_col=6+i,max_row=29,max_col=6+i):
        for cell in column:
            date_shift[i].append(cell.value) 
            
day_shift_A = list(range(15))
day_shift_AH = list(range(15))
day_shift_B = list(range(15))
day_shift_C = list(range(15))
day_shift_F = list(range(15))
day_shift_H = list(range(15))
day_shift_I = list(range(15))
day_shift_II = list(range(15))
day_shift_J = list(range(15))
for i in range(15):
    day_shift_A[i] = [] 
    day_shift_AH[i] = []
    day_shift_B[i] = []
    day_shift_C[i] = []
    day_shift_F[i] = []
    day_shift_H[i] = []  
    day_shift_I[i] = []
    day_shift_II[i] = []  
    day_shift_J[i] = []
for i in range(0,15):    
    j=0
    for column in sheet.iter_rows(min_row=4, min_col=6+i,max_row=29,max_col=6+i):
        for cell in column:
            j+=1
            if cell.value == 'A':
                day_shift_A[i].append(member_name[j-1])
            if cell.value == 'A-H':
                day_shift_AH[i].append(member_name[j-1])    
            if cell.value == 'B':
                day_shift_B[i].append(member_name[j-1])
            if cell.value == 'C':
                day_shift_C[i].append(member_name[j-1])
            if cell.value == 'F':
                day_shift_F[i].append(member_name[j-1])   
            if cell.value == 'H':
                day_shift_H[i].append(member_name[j-1]) 
            if cell.value == 'I':
                day_shift_I[i].append(member_name[j-1])
            if cell.value == 'II':
                day_shift_II[i].append(member_name[j-1])
            if cell.value == 'J':
                day_shift_J[i].append(member_name[j-1])
                
ten_A_H_day = list(range(15))
for i in range(15):
    ten_A_H_day[i] = []
ten_A_N_day = list(range(15))
