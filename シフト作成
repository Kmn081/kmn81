    ten_F_day[i] = []
ten_H_H_day = list(range(15))
for i in range(15):
    ten_H_H_day[i] = []
ten_H_N_day = list(range(15))
for i in range(15):
    ten_H_N_day[i] = []

ten_H_S_day = list(range(15))
for i in range(15):
    ten_H_S_day[i] = []        
ten_I_day = list(range(15))
for i in range(15):
    ten_I_day[i] = []
ten_HI_day = list(range(15))
for i in range(15):
    ten_HI_day[i] = []   
ten_HIJ_day = list(range(15))
for i in range(15):
    ten_HIJ_day[i] = []     
ten_II_day = list(range(15))
for i in range(15):
    ten_II_day[i] = []    
ten_J_day = list(range(15))
for i in range(15):
    ten_J_day[i] = []    
ten = list(range(100000))
for i in range(100000):
    ten[i] = []    
ten_day_shift = list(range(15))
for i in range(15):
    ten_day_shift[i] = []
        

count=0
for i in range(1,16):

    
    if day_of_the_week[i] == '土' or day_of_the_week[i] == '日':
        print('-----')
    else:  
        for j in range(0,len(day_shift_H[i-1])):
            if member_name_N[day_shift_H[i-1][j]] == 1:
                ten_H_N_day[i-1].append(day_shift_H[i-1][j])
            if member_name_S[day_shift_H[i-1][j]] == 1:
                ten_H_S_day[i-1].append(day_shift_H[i-1][j])
        for j in range(0,len(ten_H_N_day[i-1])):
            for k in range(0,len(ten_H_S_day[i-1])):
                if ten_H_S_day[i-1][k] != ten_H_N_day[i-1][j]:
                    for l in range(0,len(day_shift_H[i-1])):
                        if ten_H_S_day[i-1][k] != day_shift_H[i-1][l] and ten_H_N_day[i-1][j] != day_shift_H[i-1][l]:
                            ten_HI_day[i-1] = day_shift_I[i-1] + day_shift_H[i-1]
                            for m in range(0,len(ten_HI_day[i-1])):
                                if ten_H_S_day[i-1][k] != ten_HI_day[i-1][m] and ten_H_N_day[i-1][j] != ten_HI_day[i-1][m] and day_shift_H[i-1][l] != ten_HI_day[i-1][m]:
                                    ten_HIJ_day[i-1] = day_shift_H[i-1] + ten_HI_day[i-1]
                                    for n in range(0,len(ten_HIJ_day[i-1])):
                                        if ten_H_S_day[i-1][k] != ten_HIJ_day[i-1][n] and ten_H_N_day[i-1][j] != ten_HIJ_day[i-1][n] and day_shift_H[i-1][l] != ten_HIJ_day[i-1][n] and ten_HI_day[i-1][m] != ten_HIJ_day[i-1][n]:
                                            count += 1
                                            ten[count-1].append(ten_H_N_day[i-1][j])
                                            ten[count-1].append(ten_H_S_day[i-1][k])
                                            ten[count-1].append(day_shift_H[i-1][l])
                                            ten[count-1].append(ten_HI_day[i-1][m])
                                            ten[count-1].append(ten_HIJ_day[i-1][n])
                                            ten_day_shift[i-1].append(ten[count-1]) 
                                            
